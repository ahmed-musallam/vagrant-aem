
<IfModule dispatcher_module>
    DispatcherConfig            <%= scope['::apache::confd_dir'] %>/dispatcher.any
    DispatcherLog               <%= scope['::apache::httpd_dir'] %>/logs/dispatcher.log
    DispatcherLogLevel          1
    DispatcherNoServerHeader    1
    DispatcherDeclineRoot       0
    DispatcherUseProcessedURL   1
    DispatcherPassError         0
</IfModule>
